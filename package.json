{
  "name": "virustotal",
  "version": "1.0.0",
  "description": "virustotal api implementation in python",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icons.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"VirusTotal\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "author": "vinay raj",
  "license": "MIT",
  "dependencies": {
    "@trodi/electron-splashscreen": "^0.3.1",
    "bootstrap": "^4.1.3",
    "bootstrap-sweetalert": "^1.0.1",
    "electron": "^2.0.8",
    "jquery": "^3.3.1",
    "python-shell": "^0.5.0"
  },
  "devDependencies": {
    "electron-packager": "^12.1.1"
  }
}
